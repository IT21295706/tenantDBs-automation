<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="1" author="Ranuri">
        <addColumn tableName="example_table">
            <column name="new_column" type="varchar(255)"/>
        </addColumn>
    </changeSet>

    <changeSet id="2" author="Ranuri">
        <renameColumn
            tableName="example_table"
            oldColumnName="new_column"
            newColumnName="ranuri"
            columnDataType="varchar(255)"/>
    </changeSet>

    <changeSet id="3" author="Ranuri">
        <renameColumn
            tableName="example_table"
            oldColumnName="ranuri"
            newColumnName="ranuri1"
            columnDataType="varchar(255)"/>
    </changeSet>

    <changeSet id="4" author="Ranuri">
        <renameColumn
            tableName="example_table"
            oldColumnName="ranuri1"
            newColumnName="ranuri2"
            columnDataType="varchar(255)"/>
    </changeSet>

     <changeSet id="5" author="Ranuri">
        <renameColumn
            tableName="example_table"
            oldColumnName="ranuri2"
            newColumnName="ranuri3"
            columnDataType="varchar(255)"/>
   </changeSet>

   <changeSet id="6" author="Ranuri">
        <renameColumn
            tableName="example_table"
            oldColumnName="ranuri3"
            newColumnName="ranuri4"
            columnDataType="varchar(255)"/>
   </changeSet>

</databaseChangeLog>